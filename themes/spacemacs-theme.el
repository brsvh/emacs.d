;;; spacemacs-theme.el --- Spacemacs Themes configuration -*- lexical-binding: t; -*-

;; Disable background color of the comment lines.
(setq spacemacs-theme-comment-bg nil)

;; Show italic comment.
(setq spacemacs-theme-comment-italic t)

;; Show italic keyword
(setq spacemacs-theme-keyword-italic t)

;; Disable underline of matching parens.
(setq spacemacs-theme-underline-parens nil)

;;; spacemacs-theme.el ends here
